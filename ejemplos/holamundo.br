declare x, y :: integer; t :: tape execute
    x := 3 ;
    y := 2 ;
    t := [y];
    [3] & t; 
    $${++++++++++} at t;
    read x;
    while x > 0 do
        {+} at t;
        x := x - 1;
    done;
    while #t /= 0 do
        {>+++++++>++++++++++>+++>+<<<<-} at t
    done ;
    {>++.>+.+++++++..+++.>++.} at t;
    {<<+++++++++++++++.>.+++.} at t;
    {------.--------.>+.>.} at t;
    write t
done
